# ==========================================
# BABYSU WEB APP ENVIRONMENT CONFIGURATION
# ==========================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# ==========================================
# BACKEND API
# ==========================================
# Use your deployed backend URL in production
VITE_API_URL=http://localhost:5000/api

# ==========================================
# FIREBASE CLIENT SDK
# ==========================================
# Get these from Firebase Console: https://console.firebase.google.com/
# Project Settings > General > Your apps > Web app config
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# ==========================================
# STRIPE (CLIENT)
# ==========================================
# Get from: https://dashboard.stripe.com/test/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# ==========================================
# GOOGLE OAUTH
# ==========================================
# Get from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# ==========================================
# APP CONFIGURATION
# ==========================================
VITE_APP_NAME=BabySu
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# ==========================================
# AUTO-LOGIN (Development/Testing Only)
# ==========================================
# WARNING: Only use for development/testing! Remove in production!
VITE_AUTO_LOGIN_EMAIL=your_test_email@example.com
VITE_AUTO_LOGIN_PASSWORD=your_test_password

# ==========================================
# API KEYS (For Serverless Functions)
# ==========================================
# Only needed if deploying serverless functions

# PiAPI (if using)
VITE_PIAPI_API_KEY=your_piapi_key_here
PIAPI_API_KEY=your_piapi_key_here

# Gemini (if using)
VITE_GEMINI_API_KEY=your_gemini_key_here
GEMINI_API_KEY=your_gemini_key_here
